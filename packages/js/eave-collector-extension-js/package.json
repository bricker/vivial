{
  "name": "@eave-fyi/eave-collector-extension",
  "version": "1.0.0",
  "description": "client-side browser extension tracing",
  "type": "module",
  "main": "dist/collector.js",
  "types": "dist/collector.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "ava tests/**/*.ts",
    "build": "tsc && rollup -c --environment MODE:development,EAVE_API_BASE_URL:https://api.eave.dev",
    "build-prod": "tsc && rollup -c --environment MODE:production,EAVE_API_BASE_URL:https://api.eave.fyi",
    "prepublishOnly": "npm run build-prod"
  },
  "author": "eave.fyi",
  "license": "GPL-3.0-only",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-replace": "^6.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.0",
    "ava": "^6.1.2",
    "eslint-config-prettier": "9.x",
    "eslint-plugin-cypress": "3.x",
    "eslint-plugin-import": "2.x",
    "prettier": "3.x",
    "prettier-plugin-organize-imports": "3.x",
    "rollup": "^4.22.4",
    "sinon": "^17.0.1",
    "ts-loader": "^9.5.1"
  },
  "eslintConfig": {
    "parserOptions": {
      "project": "./tsconfig.json"
    }
  },
  "dependencies": {
    "@eave-fyi/eave-collector-core": "file:../eave-collector-core-js"
  }
