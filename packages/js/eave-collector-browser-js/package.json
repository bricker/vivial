{
  "name": "eave-collector-browser",
  "version": "1.0.0",
  "description": "client-side web tracing",
  "type": "module",
  "main": "dist/collector.js",
  "types": "dist/collector.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "npm run unit-test",
    "unit-test": "ava tests/unit/**/*.ts",
    "e2e-test": "npm run build && echo 'Copying current dist/collector.js to test app...' && cp dist/collector.js tests/integration/public/eave-client.min.js && cd tests/integration && npm test",
    "build": "webpack --mode=development",
    "build-prod": "webpack --mode=production --no-devtool",
    "prepublishOnly": "npm run build-prod"
  },
  "author": "eave.fyi",
  "license": "GPL-3.0-only",
  "devDependencies": {
    "@types/webpack": "^5.28.5",
    "ava": "^6.1.2",
    "eslint-config-prettier": "9.x",
    "eslint-plugin-cypress": "3.x",
    "eslint-plugin-import": "2.x",
    "prettier": "3.x",
    "prettier-plugin-organize-imports": "3.x",
    "sinon": "^17.0.1",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.5.1",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "eslintConfig": {
    "parserOptions": {
      "project": "./tsconfig.json"
    }
  },
  "dependencies": {
    "@eave-fyi/eave-collector-core": "file:../eave-collector-core-js",
    "@ua-parser-js/pro-business": "^2.0.0-beta.3"
  }
}
