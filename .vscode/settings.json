{
  "editor.detectIndentation": true,
  "editor.insertSpaces": true,
  "editor.indentSize": "tabSize",
  "editor.tabSize": 2,
  "files.associations": {
    "*.html.jinja": "jinja-html"
  },
  "files.exclude": {
    "**/__pycache__": true,
    "**/.build": true,
    "**/.classpath": true,
    "**/.cloudsqlproxy": true,
    "**/.factorypath": true,
    "**/.mypy_cache": true,
    "**/.project": true,
    "**/.pytest_cache": true,
    "**/.ruff_cache": true,
    "**/.settings": true,
    "**/*.egg-info": true
  },
  "files.trimFinalNewlines": false,
  "files.insertFinalNewline": false,
  "[python]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "editor.defaultFormatter": "ms-python.black-formatter",
  },
  "[shellscript]": {
    "editor.insertSpaces": false,
    "editor.tabSize": 2,
    "files.eol": "\n"
  },
  "[javascript]": {
    "editor.tabSize": 2,
  },
  "[json]": {
    "editor.tabSize": 2,
  },
  "[yaml]": {
    "editor.tabSize": 2
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "yaml.schemas": {
    "https://json.schemastore.org/github-action": ["action.yml"],
    "https://json.schemastore.org/github-workflow": ".github/workflows/*.yml",
  },
  "graphql-config.dotEnvPath": "${workspaceFolder}/.env",
  "eslint.experimental.useFlatConfig": false,
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  "eslint.validate": [
    "javascript",
    "graphql",
  ],
  "typescript.inlayHints.functionLikeReturnTypes.enabled": false,
  "typescript.inlayHints.enumMemberValues.enabled": false,
  "typescript.inlayHints.parameterNames.enabled": "none",
  "typescript.inlayHints.parameterTypes.enabled": false,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": false,
  "typescript.inlayHints.variableTypes.enabled": false,
  "typescript.updateImportsOnFileMove.enabled": "prompt",
  "javascript.inlayHints.enumMemberValues.enabled": false,
  "javascript.inlayHints.functionLikeReturnTypes.enabled": false,
  "javascript.inlayHints.parameterNames.enabled": "none",
  "javascript.inlayHints.variableTypes.suppressWhenTypeMatchesName": true,
  "python.analysis.diagnosticMode": "workspace",
  "python.analysis.typeCheckingMode": "basic",
  "python.analysis.extraPaths": [
    "vendor",
    "${workspaceFolder}/libs/eave-stdlib-py/src",
    "${workspaceFolder}/libs/eave-pubsub-schemas/python/src",
    "${workspaceFolder}/apps/core",
    "${workspaceFolder}/apps/slack",
    "${workspaceFolder}/apps/marketing",
    "${workspaceFolder}/develop/python/src",
  ],
  "python.testing.unittestEnabled": false,
  "python.testing.pytestEnabled": true,
  "python.testing.unittestArgs": [
    "-v",
    "-s",
    "${workspaceFolder}",
    "-p",
    "*test.py"
  ],
  "git.openRepositoryInParentFolders": "always",
  "python.analysis.autoImportCompletions": true,
  "python.testing.pytestArgs": [
    "-c=${workspaceFolder}/develop/python/configs/pyproject.toml",
    "--rootdir=${workspaceFolder}",
    "."
  ],
  "python.analysis.exclude": [
    "**/node_modules",
    "**/__pycache__",
    ".git",
    ".venv",
    "**/vendor",
    "**/.mypy_cache",
    "**/generated",
    "**/build",
    "**/.build",
    "**/.ruff_cache",
    "**/.pytest_cache"
  ],
  "python.analysis.importFormat": "absolute",
  "python.autoComplete.extraPaths": [
  ],
  "ruff.args": [
    "--config=${EAVE_HOME}/develop/python/configs/pyproject.toml"
  ],
  "typescript.tsserver.experimental.enableProjectDiagnostics": true,
  "markdownlint.run": "onSave",
  "markdownlint.config": {
    "first-line-heading": false,
  },
  "python.analysis.gotoDefinitionInStringLiteral": true,
  "json.schemas": [

  {
    "fileMatch": [
      "/confluence/atlassian-connect.json"
    ],
    "url": "https://bitbucket.org/atlassian/connect-schemas/raw/master/confluence-global-schema.json"
  },
  {
    "fileMatch": [
      "/jira/atlassian-connect.json"
    ],
    "url": "https://bitbucket.org/atlassian/connect-schemas/raw/master/jira-global-schema.json"
  }
  ],
  "typescript.preferences.importModuleSpecifierEnding": "js",
  "eslint.rules.customizations": [
    {
      "rule": "*",
      "severity": "downgrade"
    }
  ],
  "typescript.validate.enable": true,
  "eslint.run": "onSave",
  "ruff.importStrategy": "useBundled",
  "ruff.run": "onSave",
  "ruff.interpreter": [
    "${workspaceFolder}/.venv/bin/python"
  ],
  "todo-tree.filtering.excludeGlobs": [
    "**/node_modules/*/**",
    "**/.venv",
    "**/__pycache__",
    "**/.ruff_cache",
    "**/.mypy_cache",
    "**/.pytest_cache"
  ],
  "todo-tree.tree.autoRefresh": false,
  "shellcheck.run": "onSave",
  "shellcheck.ignorePatterns": {
    "**/.venv": true,
    "**/node_modules": true,
    "**/.ruff_cache": true,
    "**/.mypy_cache": true,
    "**/.pytest_cache": true,
    "**/__pycache__": true
  },
  "shellcheck.useWorkspaceRootAsCwd": true,
  "shellcheck.customArgs": [
    "-x"
  ],
  "black-formatter.importStrategy": "fromEnvironment",
  "black-formatter.args": [
    "--config=${workspaceFolder}/develop/python/configs/pyproject.toml"
  ],
  "prettier.configPath": "./develop/javascript/es-config/prettier/index.js",
  "prettier.ignorePath": "./develop/javascript/es-config/prettier/prettierignore",
  "graphql-config.load.rootDir": "./apps/github/graphql.config.ts",
}