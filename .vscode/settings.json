{
  "editor.detectIndentation": true,
  "editor.insertSpaces": true,
  "editor.indentSize": "tabSize",
  "editor.tabSize": 2,
  "files.associations": {
    "Fastfile": "ruby",
    "Matchfile": "ruby",
    "Pluginfile": "ruby",
    "Appfile": "ruby",
    "*.schema": "json",
    "*.swiftinterface": "swift",
    "*.html.jinja": "jinja-html",
    "*.env": "properties",
  },
  "files.exclude": {
    "**/__pycache__": true,
    "**/.build": true,
    "**/.classpath": true,
    "**/.cloudsqlproxy": true,
    "**/.factorypath": true,
    "**/.mypy_cache": true,
    "**/.project": true,
    "**/.pytest_cache": true,
    "**/.ruff_cache": true,
    "**/.settings": true,
    "**/*.egg-info": true
  },
  "files.trimFinalNewlines": false,
  "files.insertFinalNewline": false,
  "[python]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "editor.defaultFormatter": "charliermarsh.ruff",
  },
  "[shellscript]": {
    "editor.insertSpaces": false,
    "editor.tabSize": 2,
    "files.eol": "\n"
  },
  "[javascript]": {
    "editor.tabSize": 2,
  },
  "[json]": {
    "editor.tabSize": 2,
  },
  "[yaml]": {
    "editor.tabSize": 2
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "yaml.schemas": {
    "https://json.schemastore.org/github-action": [
      "action.yml"
    ],
    "https://json.schemastore.org/github-workflow": ".github/workflows/*.yml",
  },
  "eslint.experimental.useFlatConfig": false,
  "eslint.format.enable": false,
  "eslint.lintTask.enable": true,
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  "eslint.validate": [
    "javascript",
    "graphql",
  ],
  "typescript.inlayHints.functionLikeReturnTypes.enabled": false,
  "typescript.inlayHints.enumMemberValues.enabled": false,
  "typescript.inlayHints.parameterNames.enabled": "none",
  "typescript.inlayHints.parameterTypes.enabled": false,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": false,
  "typescript.inlayHints.variableTypes.enabled": false,
  "typescript.updateImportsOnFileMove.enabled": "prompt",
  "javascript.inlayHints.enumMemberValues.enabled": false,
  "javascript.inlayHints.functionLikeReturnTypes.enabled": false,
  "javascript.inlayHints.parameterNames.enabled": "none",
  "javascript.inlayHints.variableTypes.suppressWhenTypeMatchesName": true,
  "python.testing.unittestEnabled": false,
  "python.testing.pytestEnabled": true,
  "python.testing.unittestArgs": [
    "-v",
    "-s",
    "${workspaceFolder}",
    "-p",
    "*test.py"
  ],
  "git.openRepositoryInParentFolders": "always",
  "python.testing.pytestArgs": [
    "-c=${workspaceFolder}/develop/python/configs/pytest.pyproject.toml",
    "--cov-config=${workspaceFolder}/develop/python/configs/coverage.toml"
  ],
  "python.analysis.importFormat": "absolute",
  "python.autoComplete.extraPaths": [
    "${workspaceFolder}/apps/core",
    "${workspaceFolder}/apps/dashboard",
    "${workspaceFolder}/apps/admin",
    "${workspaceFolder}/develop/python/src",
    "${workspaceFolder}/libs/eave-stdlib-py/src",
  ],
  "ruff.lint.args": [
    "--config=${workspaceFolder}/develop/python/configs/ruff.toml",
    "--ignore=I"
  ],
  "typescript.tsserver.experimental.enableProjectDiagnostics": true,
  "markdownlint.run": "onSave",
  "markdownlint.config": {
    "first-line-heading": false,
  },
  "python.analysis.gotoDefinitionInStringLiteral": true,
  "json.schemas": [

  ],
  "typescript.preferences.importModuleSpecifierEnding": "auto",
  "typescript.validate.enable": true,
  "ruff.importStrategy": "fromEnvironment",
  "ruff.interpreter": [
    "${workspaceFolder}/.venv/bin/python"
  ],
  "shellcheck.run": "onSave",
  "shellcheck.ignorePatterns": {
    "**/.venv": true,
    "**/node_modules": true,
    "**/.ruff_cache": true,
    "**/.mypy_cache": true,
    "**/.pytest_cache": true,
    "**/__pycache__": true
  },
  "shellcheck.useWorkspaceRootAsCwd": true,
  "prettier.configPath": "./develop/javascript/es-config/prettier/index.js",
  "prettier.ignorePath": "./develop/javascript/es-config/prettier/prettierignore",
  "ruff.format.args": [
    "--config=${workspaceFolder}/develop/python/configs/ruff.toml"
  ],
  "ruff.organizeImports": true,
  "typescript.preferences.preferTypeOnlyAutoImports": true,
  "typescript.tsdk": "./node_modules/typescript/lib",
  "python.analysis.extraPaths": [
    "${workspaceFolder}/apps/core",
    "${workspaceFolder}/apps/dashboard",
    "${workspaceFolder}/apps/admin",
    "${workspaceFolder}/develop/python/src",
    "${workspaceFolder}/libs/eave-stdlib-py/src",
  ],
}