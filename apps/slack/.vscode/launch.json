  "version": "0.2.0",
  "configurations": [
    {
      "name": "Slack App API",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "--port",
        "5200",
        "--reload",
        "eave.slack.app:api"
      ],
      "justMyCode": false,
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "name": "Slack Socket Mode",
      "type": "python",
      "request": "launch",
      "program": "./socketmode.py",
      "autoReload": {
        "enable": true,
        "include": [
          "**/*.py",
        ]
      },
      "console": "integratedTerminal",
      "justMyCode": true,
      "envFile": "${workspaceFolder}/.env"
    },
  ]
}
