#!/usr/bin/env sh

echo "This script requires the gcloud SDK to be installed and in your path"
echo "Additionally, a python2 program must be installed and in your PATH."
echo "https://cloud.google.com/appengine/docs/legacy/standard/python/tools/using-local-server"

set -o allexport
source .env
set +o allexport

dev_appserver.py \
    --host localhost \
    --port 5300 \
    app.yaml
